<%= form_for(@car_brand) do |f| %>
  <% if @car_brand.errors.any? %>

  <div class="field">
    <%= f.label :car_origin_id %><br />
    <%= f.number_field :car_origin_id %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
